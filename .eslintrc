{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],

  "rules": {
    "padding-line-between-statements": [
        "error",
        { "blankLine": "always", "prev": "multiline-expression", "next": "multiline-expression" }
    ]
  }
}
